{
  "version": 1,
  "plugins": {
    "net": {
      "interfaces": [
        {
          "name": "ovsbr0",
          "type": "ovs-bridge",
          "ports": ["ens1"],
          "ipv4": {
            "enabled": true,
            "dhcp": false,
            "address": [
              {
                "ip": "172.16.0.10",
                "prefix": 24
              }
            ],
            "gateway": "172.16.0.1"
          }
        }
      ]
    },
    "systemd": {
      "units": {
        "openvswitch-switch.service": {
          "active_state": "active",
          "enabled": true
        }
      }
    }
  }
}
